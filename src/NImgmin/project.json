{
    "title": "NImgmin",
    "authors": [ "Mike Morano <mmorano@mikeandwan.us>" ],
    "description": ".Net wrapper for imgmin",
    "version": "0.2.1",
    "packOptions": {
        "licenseUrl": "https://github.com/AerisG222/NImgmin/blob/master/LICENSE.md",
